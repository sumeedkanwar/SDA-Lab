package Main;
import java.util.Scanner;

public class Main {
	
	public static void main(String [] args)
	{
		int size = 5;
		int [] num;
		Scanner input = new Scanner(System.in);
		
		
		for (int number: size)
		{
			System.out.println("Input number");
			num[number] = input.nextInt();
			
		}
		
		System.out.println("dasd");
	}

}
